{
  "name": "nisomnia",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "check": "bunx --bun astro check",
    "dev": "bunx --bun astro dev",
    "build": "bunx --bun astro build",
    "preview": "bunx --bun astro preview --host 0.0.0.0",
    "astro": "bunx --bun astro",
    "start": "bunx --bun run ./dist/server/entry.mjs --host 0.0.0.0",
    "db:check": "drizzle-kit check",
    "db:drop": "drizzle-kit drop",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "npx tsx -r dotenv/config ./src/server/db/migrate.ts",
    "db:pull": "drizzle-kit introspect",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:up": "drizzle-kit up",
    "format:check": "prettier --check \"**/*.{astro,ts,tsx,mts,js,jsx,mjs,json,md}\"",
    "format:write": "prettier --write \"**/*.{astro,ts,tsx,mts,js,jsx,mjs,json,md}\"",
    "lint": "eslint --ext .astro,.ts,.tsx,.js,.mjs ./src",
    "lint:fix": "eslint --ext .astro,.ts,.tsx,.js,.mjs ./src --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/node": "^9.1.2",
    "@astrojs/ts-plugin": "^1.10.4",
    "@fontsource/inter": "^5.2.5",
    "@tailwindcss/vite": "^4.0.11",
    "@trpc/client": "^11.0.0-rc.824",
    "@trpc/server": "^11.0.0-rc.824",
    "@yopem-ui/utils": "^0.0.1",
    "@yopem/try-catch": "^0.0.1",
    "astro": "^5.4.2",
    "astro-seo-meta": "^5.1.0",
    "astro-seo-schema": "^5.0.0",
    "drizzle-orm": "^0.40.0",
    "drizzle-zod": "^0.7.0",
    "nanoid": "^5.1.2",
    "node-html-parser": "^7.0.1",
    "superjson": "^2.2.2",
    "tailwindcss": "^4.0.12",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.21.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/bun": "^1.2.4",
    "@typescript-eslint/parser": "^8.26.0",
    "drizzle-kit": "^0.30.5",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "lefthook": "^1.11.3",
    "pg": "^8.14.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0"
  },
  "packageManager": "bun@1.2.5"
}
