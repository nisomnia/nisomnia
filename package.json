{
  "name": "nisomnia",
  "type": "module",
  "version": "0.0.1",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "check": "bunx --bun astro check",
    "dev": "bunx --bun astro dev",
    "build": "bunx --bun astro build",
    "preview": "bunx --bun astro preview --host 0.0.0.0",
    "astro": "bunx --bun astro",
    "start": "bunx --bun run ./dist/server/entry.mjs --host 0.0.0.0",
    "db:check": "drizzle-kit check",
    "db:drop": "drizzle-kit drop",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:pull": "drizzle-kit introspect",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:up": "drizzle-kit up",
    "format:check": "prettier --check \"**/*.{astro,ts,tsx,mts,js,jsx,mjs,json,md}\"",
    "format:write": "prettier --write \"**/*.{astro,ts,tsx,mts,js,jsx,mjs,json,md}\"",
    "lint": "eslint --ext .astro,.ts,.tsx,.js,.mjs ./src",
    "lint:fix": "eslint --ext .astro,.ts,.tsx,.js,.mjs ./src --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/node": "9.3.3",
    "@astrojs/ts-plugin": "^1.10.4",
    "@fontsource/inter": "^5.2.6",
    "@tailwindcss/vite": "^4.1.11",
    "@trpc/client": "^11.4.4",
    "@trpc/server": "^11.4.4",
    "@yopem-ui/utils": "^0.0.2",
    "@yopem/try-catch": "^0.0.2",
    "astro": "5.12.8",
    "astro-embed": "^0.9.0",
    "astro-seo-meta": "^5.2.0",
    "astro-seo-schema": "^5.1.0",
    "dayjs": "^1.11.13",
    "drizzle-orm": "^0.43.1",
    "drizzle-zod": "^0.7.1",
    "nanoid": "^5.1.5",
    "node-html-parser": "^7.0.1",
    "superjson": "^2.2.2",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.32.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.5.1",
    "@types/bun": "^1.2.19",
    "@typescript-eslint/parser": "^8.39.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "lefthook": "^1.12.2",
    "pg": "^8.16.3",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  },
  "packageManager": "bun@1.2.19"
