{
  "name": "nisomnia",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "bunx --bun astro dev",
    "build": "bunx --bun astro build",
    "preview": "bunx --bun astro preview",
    "astro": "bunx --bun astro",
    "start": "bunx --bun run ./dist/server/entry.mjs",
    "lint": "eslint --ext .astro,.ts,.tsx,.js,.mjs ./src",
    "lint:fix": "eslint --ext .astro,.ts,.tsx,.js,.mjs ./src --fix",
    "format:check": "prettier --check \"**/*.{astro,ts,tsx,js,mjs,md}\"",
    "format:write": "prettier --write \"**/*.{astro,ts,tsx,js,mjs,md}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@astrojs/node": "^9.1.2",
    "@astrojs/ts-plugin": "^1.10.4",
    "@fontsource/inter": "^5.2.5",
    "@tailwindcss/vite": "^4.0.9",
    "@trpc/client": "^11.0.0-rc.824",
    "@trpc/server": "^11.0.0-rc.824",
    "@yopem-ui/utils": "^0.0.1",
    "@yopem/try-catch": "^0.0.1",
    "astro": "^5.4.2",
    "drizzle-orm": "^0.40.0",
    "drizzle-zod": "^0.7.0",
    "nanoid": "^5.1.2",
    "superjson": "^2.2.2",
    "tailwindcss": "^4.0.9",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.21.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/bun": "^1.2.4",
    "@typescript-eslint/parser": "^8.26.0",
    "drizzle-kit": "^0.30.5",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "lefthook": "^1.11.2",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript-eslint": "^8.26.0"
  },
  "packageManager": "bun@1.2.4"
}
